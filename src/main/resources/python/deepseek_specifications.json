[
  {
    "name": "Path do prompt de instrução",
    "argName": "--prompt_instruction_path",
    "type": "string",
    "required": true,
    "ui_element": "filePicker",
    "description": "Caminho do arquivo contendo as instruções do prompt para o modelo",
    "default_value": ""
  },
  {
    "name": "Chave API",
    "argName": "--api_key",
    "type": "string",
    "required": true,
    "ui_element": "textfield",
    "description": "Chave da API do DeepSeek",
    "default_value": ""
  },
  {
    "name": "Output dir",
    "argName": "--output_dir_path",
    "type": "string",
    "required": true,
    "ui_element": "filePicker",
    "description": "Diretório onde a saída gerada pelo modelo será salva",
    "default_value": ""
  },
  {
    "name": "Temperatura",
    "argName": "--temperature",
    "type": "float",
    "required": true,
    "ui_element": "spinner",
    "description": "Define a temperatura do modelo, afetando a aleatoriedade da resposta",
    "allowed_values": {
      "min": 0.0,
      "max": 1.0
    },
    "default_value": 0.7,
    "step": 0.1
  },
  {
    "name": "Debug",
    "argName": "--debug",
    "type": "boolean",
    "required": false,
    "ui_element": "checkbox",
    "description": "Ativa logs detalhados durante a execução",
    "default_value": false
  },
  {
    "name": "Modelo",
    "argName": "--model",
    "type": "string",
    "required": true,
    "ui_element": "combobox",
    "description": "Modelo do DeepSeek a ser utilizado",
    "allowed_values": ["deepseek-chat", "deepseek-coder"],
    "default_value": "deepseek-chat"
  }
]
