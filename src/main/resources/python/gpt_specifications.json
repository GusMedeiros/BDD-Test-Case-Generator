    {
        "name": "Debug",
        "type": "boolean",
        "required": false,
        "ui_element": "checkbox",
        "description": "Whether to run the script in debug mode",
        "default_value": false
    },
    {
        "name": "Modelo",
        "type": "string",
        "required": true,
        "ui_element": "combobox",
        "description": "The model to use for generating completions",
        "allowed_values": ["gpt-3.5-turbo", "gpt-4", "gpt-4-32k"],
        "default_value": "gpt-3.5-turbo"
