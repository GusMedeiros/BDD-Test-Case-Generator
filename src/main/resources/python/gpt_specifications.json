[
    {
        "name": "Path do prompt de instrução",
        "type": "string",
        "required": true,
        "ui_element": "filePicker",
        "description": "Path to the predefined instruction to be applied to the user story",
        "default_value": ""
    },
    {
        "name": "Path História de Usuário",
        "type": "string",
        "required": true,
        "ui_element": "filePicker",
        "description": "Path to the user story",
        "default_value": ""
    },
    {
        "name": "Chave API",
        "type": "string",
        "required": true,
        "ui_element": "textfield",
        "description": "OpenAI API key",
        "default_value": ""
    },
    {
        "name": "Output dir",
        "type": "string",
        "required": true,
        "ui_element": "filePicker",
        "description": "Path to the output directory",
        "default_value": ""
    },
    {
        "name": "Temperatura",
        "type": "float",
        "required": true,
        "ui_element": "spinner",
